.part_links
	border-radius 3px
	border 1px solid basic-border-color
	padding 0.5em
	margin 1em 0
	background-color rgba(#fff, 0.8)
	border 1px solid #D9D9D9
	a
		margin-right 1em
	h4
		margin 0.3em 0
		font-size 0.95em

.documentation_part
	font-size 0.9em
	margin-top 1em
	border-radius 3px
	border 1px solid #D9D9D9
	padding 0 1em 0 1em
	background-color rgba(#fff, 0.8)
	img
		border 1px solid #d1d1d1
		border-radius 2px
		margin-bottom 1.5em
		&.image_float
			float left
			margin-right 1em
		&.image_fullwidth
			display block
			width 653px
	h3
		margin-bottom 1.5em
		margin-top 0.5em
		a
			color dokumentation-color-font
			text-decoration none
			background-image none
			&.active 
				background-color #dee4f2
	.part_content.margin_left_class
		margin-left 220px	
